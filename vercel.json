{
  "version": 2,
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/products(.*)",
      "dest": "/api/products.js"
    },
    {
      "src": "/api/orders(.*)",
      "dest": "/api/orders.js"
    },
    {
      "src": "/api/auth(.*)",
      "dest": "/api/auth.js"
    },
    {
      "src": "/api/categories(.*)",
      "dest": "/api/categories.js"
    },
    {
      "src": "/api/health",
      "dest": "/api/health.js"
    }
  ]
}
